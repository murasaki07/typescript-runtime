{
  "name": "typescript-runtime",
  "version": "1.0.0",
  "description": "TypeScript runtime environment",
  "main": "dist/index.js",
  "packageManager": "pnpm@10.9.0",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "watch": "tsc --watch",
    "clean": "rm -rf dist",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts"
  },
  "keywords": [
    "typescript",
    "runtime"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^20.19.8",
    "@typescript-eslint/eslint-plugin": "^8.37.0",
    "@typescript-eslint/parser": "^8.37.0",
    "eslint": "^8.57.1",
    "jest": "^29.7.0",
    "prettier": "3.2.5",
    "ts-jest": "^29.4.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.3"
  },
  "engines": {
    "node": "^20.0.0",
    "pnpm": ">=8.0.0"
  },
  "dependencies": {
    "@ai-sdk/google": "^1.2.22",
    "@ai-sdk/google-vertex": "^2.2.27",
    "@ai-sdk/openai": "^1.3.23",
    "@azure/ai-projects": "^1.0.0",
    "@azure/identity": "^4.12.0",
    "@azure/openai": "^2.0.0",
    "@google/genai": "^1.9.0",
    "ai": "^4.3.19",
    "anthropic-vertex-ai": "^1.0.2",
    "dotenv": "^16.6.1",
    "openai": "^5.23.0",
    "tsx": "^4.20.5",
    "zod": "^3.25.76",
    "zod-to-json-schema": "^3.24.6"
  }
}
